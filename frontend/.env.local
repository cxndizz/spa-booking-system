# üåê Frontend Environment Variables

# API Backend URL
VITE_API_URL=http://localhost:3000/api/v1

# APP Information
VITE_APP_NAME="Spa Booking System"
VITE_APP_VERSION=1.0.0

# Business Information (for display)
VITE_BUSINESS_NAME="Your Spa Name"
VITE_BUSINESS_PHONE="+66-xx-xxx-xxxx"

# Deployment URLs (for different environments)
VITE_FRONTEND_URL=https://your-admin.vercel.app

# Development settings
VITE_DEBUG_MODE=true
VITE_MOCK_API=false

# Feature flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_PWA=true

# Payment (Omise) - Public key only
VITE_OMISE_PUBLIC_KEY=your-omise-public-key

# Google Maps (if needed for location services)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
